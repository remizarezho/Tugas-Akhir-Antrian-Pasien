<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    android:id="@+id/relativeRuangan"
    android:orientation="vertical">

    <TextView
        android:id="@+id/list_ruangperiksa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Ruang Periksa"
        android:textSize="45dp"
        android:layout_marginBottom="20dp"/>

    <android.support.v7.widget.RecyclerView
        android:layout_below="@id/list_ruangperiksa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recycleview1"/>

    <ImageView
        android:id="@+id/prefences2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="@drawable/backgroundlingkaranpink"
        android:padding="10dp"
        android:src="@drawable/ic_playlist_add_black_24dp"/>


</RelativeLayout>